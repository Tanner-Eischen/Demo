# Z.ai (OpenAI-compatible) settings
ZAI_API_KEY=
ZAI_BASE_URL=https://api.z.ai/api/paas/v4/
ZAI_VISION_MODEL=glm-4.6v
ZAI_REWRITE_MODEL=glm-5

# TTS endpoint (optional)
# Examples:
# - http://host.docker.internal:5001/tts
# - http://tts:5001/tts (if you add a tts service)
TTS_ENDPOINT=
TTS_MODE=chatterbox_tts_json  # chatterbox_tts_json | openai_audio_speech

# Redis / RQ
REDIS_URL=redis://redis:6379/0
RQ_QUEUE=default

# Demo capture runtime
DEMO_CAPTURE_EXECUTION_MODE=playwright_optional  # playwright_optional | playwright_required
INSTALL_PLAYWRIGHT=0  # set to 1 to install Playwright + Chromium during image build

# Storage
DATA_DIR=/data
